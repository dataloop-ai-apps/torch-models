{
  "name": "resnet",
  "displayName": "ResNet",
  "version": "0.1.1",
  "scope": "public",
  "description": "ResNet Model Application",
  "attributes": {
    "Provider": "Dataloop",
    "Category": "Model",
    "Computer Vision": "Classification",
    "Media Type": "Image",
    "Libraries": "Torch"
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/torch-models.git",
    "gitTag": "0.1.1"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "resnet-deploy",
        "runtime": {
          "podType": "regular-xs",
          "concurrency": 1,
          "runnerImage": "gcr.io/viewo-g/modelmgmt/resnet:0.0.8",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 100
          }
        }
      },
      {
        "name": "resnet-train-evaluate",
        "driverId": "",
        "runtime": {
          "podType": "gpu-t4",
          "concurrency": 1,
          "runnerImage": "gcr.io/viewo-g/modelmgmt/resnet:0.0.8",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 100
          }
        }
      }
    ],
    "modules": [
      {
        "name": "resnet-module",
        "entryPoint": "resnet_adapter.py",
        "className": "ModelAdapter",
        "computeConfig": "resnet-deploy",
        "description": "ResNet Module",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "evaluate_model",
            "computeConfig": "resnet-train-evaluate",
            "input": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              }
            ],
            "output": [],
            "displayName": "Evaluate a Model",
            "displayIcon": "",
            "description": "Function to evaluate ResNet model performance"
          },
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "List of items to run inference on"
              }
            ],
            "output": [],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "Function to run ResNet inference on items"
          },
          {
            "name": "train_model",
            "computeConfig": "resnet-train-evaluate",
            "input": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              }
            ],
            "output": [],
            "displayName": "Train a Model",
            "displayIcon": "",
            "description": "Function to train ResNet model"
          }
        ]
      }
    ],
    "models": [
      {
        "name": "pretrained-resnet",
        "moduleName": "resnet-module",
        "scope": "project",
        "status": "trained",
        "configuration": {
          "weights_filename": "model.pth",
          "batch_size": 16,
          "num_epochs": 10
        },
        "artifacts": [
          {
            "type": "link",
            "url": "https://storage.googleapis.com/model-mgmt-snapshots/ResNet50/model.pth"
          }
        ],
        "inputType": "image",
        "supportedMethods": {
          "load": true,
          "predict": true,
          "train": true,
          "evaluate": true
        },
        "description": "Resnet model from The torchvision package consists of popular datasets, model architectures, and common image transformations for computer vision"
      }
    ]
  }
}
